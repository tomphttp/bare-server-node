{
	"compilerOptions": {
		"outDir": "dist",
		"lib": ["ESNext", "DOM", "DOM.Iterable"],
		"target": "ESNext",
		"module": "CommonJS",
		"moduleResolution": "Node16",
		"strict": true,
		"stripInternal": true,
		"esModuleInterop": true,
		"declaration": true,
		"sourceMap": true,
		"resolveJsonModule": false
	},
	"include": ["src"]
}
